{
  "name": "flowdiag",
  "displayName": "flowdiag",
  "publisher": "akshatsinha0",
  "description": "The visual representation diagrams for the programming functions",
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/akshatsinha0/flowdiag.git"
  },
  "activationEvents": [
    "onCommand:flowdiag.debugParse",
    "onCommand:flowdiag.showFlowchart",
    "onCommand:flowdiag.test",
    "onCommand:flowdiag.retryInit"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      
      { "command": "flowdiag.debugParse", "title": "FlowDiag: Debug Parse" },
      { "command": "flowdiag.showFlowchart", "title": "FlowDiag: Show Flowchart" },
      { "command": "flowdiag.test", "title": "FlowDiag: Test Extension" },
      { "command": "flowdiag.retryInit", "title": "FlowDiag: Retry Initialization" }
      
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/emscripten": "^1.40.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.102.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.25.3",
    "eslint": "^9.25.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "web-tree-sitter": "^0.25.8"
  }
}
